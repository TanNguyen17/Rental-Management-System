<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="720.0" prefWidth="921.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yourcompany.rentalmanagement.view.RentalAgreementManagementView">
   <children>
      <VBox spacing="10.0" stylesheets="@../css/rental-agreement.css">
         <children>
            <TableView fx:id="rentalAgreementTableView" prefHeight="574.0" prefWidth="953.0" styleClass="table-view" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="agreementId" prefWidth="75.0" styleClass="table-column" text="ID" />
                  <TableColumn fx:id="status" prefWidth="100.0" styleClass="table-column-status" text="STATUS" />
                  <TableColumn fx:id="contractedDate" prefWidth="120.0" styleClass="table-column" text="CONTRACT DATE" />
                  <TableColumn fx:id="owner" prefWidth="100.0" styleClass="table-column" text="OWNER" />
                  <TableColumn fx:id="host" prefWidth="100.0" styleClass="table-column" text="HOST" />
                  <TableColumn fx:id="rentingFee" prefWidth="100.0" styleClass="table-column" text="RENTING FEE" />
                  <TableColumn fx:id="view" prefWidth="100.0" styleClass="table-column-actions" text="VIEW" />
                  <TableColumn fx:id="delete" prefWidth="100.0" styleClass="table-column-actions" text="DELETE" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>