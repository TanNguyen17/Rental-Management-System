<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.yourcompany.rentalmanagement.controller.LoginController">
    <children>
        <Label text="Login" layoutX="150" layoutY="50" />
        <TextField fx:id="usernameField" promptText="Username" layoutX="100" layoutY="100" />
        <PasswordField fx:id="passwordField" promptText="Password" layoutX="100" layoutY="150" />
        <Button text="Login" layoutX="150" layoutY="200" onAction="#handleLogin" />
    </children>
</AnchorPane>
