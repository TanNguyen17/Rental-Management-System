<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="208.0" spacing="12.0" stylesheets="@../../../css/rental-agreement.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yourcompany.rentalmanagement.view.RentalAgreementManagementView">
    <children>
        <Button fx:id="addNewBtn" mnemonicParsing="false" text="Add New Data" />
        <TableView fx:id="rentalAgreementTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"  prefHeight="718.0" prefWidth="1080.0">
            <columns>
                <TableColumn fx:id="agreementId" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120" style="-fx-alignment: CENTER;" text="AGREEMENT ID" styleClass="table-column"/>
                <TableColumn fx:id="status" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="STATUS" styleClass="table-column-status"/>
                <TableColumn fx:id="startDate" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="START DATE" styleClass="table-column"/>
                <TableColumn fx:id="endDate" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="END DATE" styleClass="table-column"/>
                <TableColumn fx:id="rentingFee" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="RENTING FEE" styleClass="table-column"/>
                <TableColumn fx:id="host" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="HOST" styleClass="table-column"/>
                <TableColumn fx:id="owner" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="OWNER" styleClass="table-column"/>
                <TableColumn fx:id="view" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="VIEW" styleClass="table-column-actions"/>
                <TableColumn fx:id="delete" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" style="-fx-alignment: CENTER;" text="DELETE" styleClass="table-column-actions"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </children>
    <padding>
        <Insets right="8.0" top="8.0" />
    </padding>
</VBox>